    {
      "type": "MIT",
      "url": "https://github.com/daB0bby/electron-angular-boilerplate/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/daB0bby/electron-angular-boilerplate#readme",
  "dependencies": {
    "@angular/animations": "^4.4.2",
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/common": "^4.4.2",
    "@angular/compiler": "^4.4.2",
    "@angular/core": "^4.4.2",
    "@angular/forms": "^4.4.2",
    "@angular/http": "^4.4.2",
    "@angular/material": "^2.0.0-beta.10",
    "@angular/platform-browser": "^4.4.2",
    "@angular/platform-browser-dynamic": "^4.4.2",
