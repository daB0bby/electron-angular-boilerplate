    "asar": "^0.14.3",
    "awesome-typescript-loader": "^5.0.0-1",
    "css-loader": "^0.28.11",
    "electron-reload": "^1.2.2",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
