    {
      "type": "MIT",
      "url": "https://github.com/daB0bby/electron-angular-boilerplate/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/daB0bby/electron-angular-boilerplate#readme",
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/forms": "^6.1.7",
    "@angular/http": "^6.1.7",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
