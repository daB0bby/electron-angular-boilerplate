    "asar": "^0.14.0",
    "awesome-typescript-loader": "^3.3.0",
    "css-loader": "^0.28.7",
    "electron-reload": "^1.2.2",
    "eslint": "^4.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
